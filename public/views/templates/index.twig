{% extends 'layout.twig' %}
{% block content %}
    <div class="container">
        <h4 class="text-center mt-5">{{ message }}</h4>
        <div class="d-flex justify-content-center">
            <input type="button" id="btnTest" value="Testear funcionamiento" class="btn btn-success">
        </div>

        <div class="form-group">
            <input type="text" class="form-control" id="email" placeholder="Email" >
            <input type="hidden" class="form-control" id="token" value="{{ token }}">
        </div>

    </div>
{% endblock %}